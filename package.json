    "globals": "^17.3.0",
    "html-webpack-plugin": "^5.5.3",
    "prettier": "^3.8.1",
    "rimraf": "^5.0.1",
    "style-loader": "^3.3.3",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^6.0.1"
  }
